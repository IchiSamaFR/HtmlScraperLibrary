<?xml version="1.0" encoding="utf-8" ?>
<root>
	<select outputKey="lst" isArray="true">
		<loop name="page" from="0" to="2" contextStepKey="step" outputKey="loop{step}">
			<select query=".lst p" isArray="true">
				<text>
					<replace oldValue="te" newValue="TE" />
					<prefix value="Item: " />
					<suffix value=";" />
				</text>
			</select>
		</loop>
	</select>
</root>