<?xml version="1.0" encoding="utf-8" ?>
<Root>
	<Select OutputKey="lst" IsArray="true">
		<Loop Name="loop" From="0" To="2" OutputKey="loop{loop.ActualStep}">
			<Select Query=".lst p" IsArray="true">
				<Text>
					<Replace OldValue="te" NewValue="TE" />
					<Prefix Value="Item: " />
					<Suffix Value=";" />
				</Text>
			</Select>
		</Loop>
	</Select>
</Root>